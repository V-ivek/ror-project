<h2>Welcome to Users controller</h2>
<h1>Listing Users</h1>
<table>
  <tr>
    <th>Username</th>
    <th>email</th>
    <th>password</th>
    <th></th>
    
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.password %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table> 
<%= link_to 'New user', new_user_path %>
<%= link_to 'Back', controller: 'welcome' %>